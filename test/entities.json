      "AGRKB",
      "SPDI",
      "WB",
      "WormBase",
      "PHARMGKB.VARIANT"
    ],
    "alt_descriptions": {
      "AGR": "An entity that describes a single affected, endogenous allele. These can be of any type that matches that definition",
      "VMC": "A contiguous change at a Location"
    },
    "slots": ["has gene"],
    "slot_usage": {
      "has gene": {
        "multivalued": true,
        "description": "Each allele can be associated with any number of genes"
      },
      "has biological sequence": {
        "description": "The state of the sequence w.r.t a reference sequence"
