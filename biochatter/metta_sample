***

; Get properties of a 'ontology_term' with some id <ontology_term_id>: 
                                        ($prop (ontology_term <ontology_term_id>) $val)
                                        ($prop $val)

; Get the 'term_name' property of some 'ontology_term' with id <ontology_term_id>: 
                                            (term_name (ontology_term <ontology_term_id>) $val)
                                            ($val)

; Get the properties of a 'ontology_term' with 'term_name' of <some_term_name_val>: 
                                            (,
                                             (term_name (ontology_term $id) <some_term_name_val>)
                                             ($prop (ontology_term $id) $val)
                                            )
                                            ($prop $val)

; Get the 'description' property of some 'ontology_term' with id <ontology_term_id>: 
                                            (description (ontology_term <ontology_term_id>) $val)
                                            ($val)

; Get the properties of a 'ontology_term' with 'description' of <some_description_val>: 
                                            (,
                                             (description (ontology_term $id) <some_description_val>)
                                             ($prop (ontology_term $id) $val)
                                            )
                                            ($prop $val)

; Get the 'synonyms' property of some 'ontology_term' with id <ontology_term_id>: 
                                            (synonyms (ontology_term <ontology_term_id>) $val)
                                            ($val)

; Get the properties of a 'ontology_term' with 'synonyms' of <some_synonyms_val>: 
                                            (,
                                             (synonyms (ontology_term $id) <some_synonyms_val>)
                                             ($prop (ontology_term $id) $val)
                                            )
                                            ($prop $val)

